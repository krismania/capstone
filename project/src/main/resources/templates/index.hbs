<html>
    <head>
        <title>Rebu - Homepage</title>
        {{!-- Favicons --}}
        <link rel="apple-touch-icon" sizes="180x180" href="/img/icons/apple-touch-icon.png">
        <link rel="icon" type="image/png" sizes="32x32" href="/img/icons/favicon-32x32.png">
        <link rel="icon" type="image/png" sizes="16x16" href="/img/icons/favicon-16x16.png">
        <link rel="manifest" href="/img/icons/site.webmanifest">
        <link rel="mask-icon" href="/img/icons/safari-pinned-tab.svg" color="#262626">
        <link rel="shortcut icon" href="/img/icons/favicon.ico">
        <meta name="msapplication-TileColor" content="#262626">
        <meta name="msapplication-config" content="/img/icons/browserconfig.xml">
        <meta name="theme-color" content="#262626">
        {{!-- End Favicons --}}
        <meta name="viewport" content="initial-scale=1.0">
        <meta charset="utf-8">
        <link rel="stylesheet" href="css/main.css">
        <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    </head>
    <body>
        <header>
            <h1>Rebu</h1>
        </header>
        <div id="map-wrapper">
            <div id="map"></div>
        </div>
        <form id="search-box">
            <input id="search-input" type="text" placeholder="Find a car near...">
            <button id="search-button"><i class="material-icons">search</i></button>
            <button id="geo-button"><i class="material-icons">my_location</i></button>
        </form>
        <script src="js/main.js" defer></script>
        <script src="https://maps.googleapis.com/maps/api/js?key={{mapsApiKey}}&callback=initMap" async defer></script>
    </body>
</html>
